{
  "name": "tachyon-ui",
  "version": "0.0.1",
  "description": "A TypeScript/React UI component library that imitates the interface of the game Starsector.",
  "author": {
    "name": "jn_xyp",
    "email": "jinan.xyp@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jnxyp/tachyon-ui.git"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "stylelint": "stylelint **/*.css",
    "stylelint:fix": "stylelint **/*.css --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "vitest run && npm run build"
  },
  "module": "./dist/main.js",
  "exports": "./dist/main.js",
  "files": [
    "dist"
  ],
  "types": "./dist/main.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.3",
    "@emotion/babel-plugin": "^11.13.5",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/addon-themes": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@testing-library/react": "16.1.0",
    "@types/node": "22.10.2",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "@typescript-eslint/eslint-plugin": "8.18.2",
    "@typescript-eslint/parser": "8.18.2",
    "@vitejs/plugin-react": "4.3.4",
    "@vitest/coverage-v8": "^3.1.2",
    "@vitest/ui": "^3.1.2",
    "autoprefixer": "10.4.20",
    "clsx": "2.1.1",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.16",
    "eslint-plugin-storybook": "0.11.1",
    "glob": "11.0.0",
    "jsdom": "25.0.1",
    "prettier": "3.4.2",
    "storybook": "^8.6.12",
    "stylelint": "16.12.0",
    "stylelint-config-standard": "36.0.1",
    "typescript": "5.7.2",
    "vite": "^6.3.4",
    "vite-plugin-dts": "4.4.0",
    "vite-plugin-lib-inject-css": "2.2.0",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "csstype": "^3.1.3"
  }
}
